<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="deep_teal_200">#80cbc4</color>
    <color name="deep_teal_500">#009688</color>
    <color name="deep_teal_700">#00796b</color>

    <!-- Base application theme. -->
    <style name="MyAppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/deep_teal_500</item>
        <item name="colorPrimaryDark">@color/deep_teal_700</item>
