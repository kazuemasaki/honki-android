<resources>
    <color name="deep_teal_200">#80cbc4</color>
    <color name="deep_teal_500">#009688</color>
    <color name="deep_teal_700">#00796b</color>

    <!-- Base application theme. -->
    <style name="MyAppTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/deep_teal_500</item>
        <item name="colorPrimaryDark">@color/deep_teal_700</item>
    </style>


    <!--アクセントカラー-->
    <color name="material_grey_600">#757575</color>
    <color name="colorAccent">#cddc39</color>

    <!--計算Buttonのスタイル-->
    <style name="calcButton" parent="android:Widget.Button">
        <item name="android:background">@drawable/selector_button</item>
        <item name="android:textSize">32sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">normal</item>
    </style>

</resources>
